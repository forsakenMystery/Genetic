   <left>
      <AnchorPane fx:id="pane" maxHeight="520.0" maxWidth="520.0" minHeight="520.0" minWidth="520.0" prefHeight="520.0" prefWidth="520.0" BorderPane.alignment="CENTER" />
   </left>
   <top>
      <Label fx:id="lbl" text="Label" BorderPane.alignment="CENTER_LEFT" />
   </top>
   <bottom>
      <HBox BorderPane.alignment="BOTTOM_LEFT">
         <children>
            <ImageView fx:id="play" fitHeight="50.0" fitWidth="50.0" onMouseClicked="#playClicked" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="15.0" right="15.0" />
               </HBox.margin>
               <image>
                  <Image url="@play.png" />
               </image>
            </ImageView>
            <ImageView fx:id="step" fitHeight="50.0" fitWidth="50.0" onMouseClicked="#stepClicked" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets left="15.0" right="15.0" />
               </HBox.margin>
               <image>
                  <Image url="@step.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </bottom>
</BorderPane>
